{
  "name": "marp-vue-slide",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pdf": "marp ./public/slide.md -o ./dist/slide.pdf",
    "og-image": "marp ./public/slide.md -o ./dist/slide.jpg",
    "netlify": "yarn pdf && yarn og-image"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "marp": {
    "allowLocalFiles": true
  },
  "dependencies": {
    "@marp-team/marp-core": "^0.11.0",
    "@marp-team/marp-vue": "^0.0.1",
    "core-js": "^2.6.5",
    "screenfull": "^4.2.0",
    "swiper": "^4.5.0",
    "vue": "^2.6.6",
    "vue-awesome-swiper": "^3.1.3",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^0.11.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "raw-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
